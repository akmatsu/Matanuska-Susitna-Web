{
  "name": "cms",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "dev": "keystone dev --no-db-push",
    "start": "keystone start",
    "build": "keystone build && keystone prisma migrate deploy",
    "migrate": "keystone prisma migrate deploy",
    "fix": "keystone postinstall --fix",
    "generate": "keystone prisma generate --binary-targets native,rhel-openssl-3.0.x",
    "migrations": "keystone prisma migrate dev --create-only",
    "reset": "keystone prisma migrate reset"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.14.1",
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/core": "^6.3.0",
    "@keystone-6/fields-document": "^9.1.0",
    "@toast-ui/editor": "^3.2.2",
    "dotenv": "^16.4.5",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@types/passport": "^1.0.16"
  }
}
