{
  "name": "cms",
  "version": "1.0.3",
  "private": true,
  "module": "ESNext",
  "scripts": {
    "dev": "keystone dev --no-db-push",
    "start": "keystone start",
    "build": "keystone build",
    "migrate": "keystone prisma migrate deploy",
    "fix": "keystone postinstall --fix",
    "generate": "keystone prisma generate --binary-targets native,rhel-openssl-3.0.x",
    "migrations": "keystone prisma migrate dev --create-only",
    "reset": "keystone prisma migrate reset",
    "collections:create": "npx ts-node ./scripts/createCollections.ts",
    "collections:remove": "npx ts-node ./scripts/removeCollection.ts",
    "typesense:import:services": "npx ts-node ./scripts/importServices.ts",
    "typesense:import:communities": "npx ts-node ./scripts/importCommunities.ts"
  },
  "dependencies": {
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/core": "^6.3.0",
    "@keystone-6/fields-document": "^9.1.0",
    "@keystone-ui/button": "^7.0.2",
    "@keystone-ui/fields": "^7.2.0",
    "@msb/md-editor": "workspace:*",
    "@prosemirror-adapter/react": "^0.2.6",
    "@types/express-session": "^1.18.0",
    "dotenv": "^16.4.5",
    "express-session": "^1.18.0",
    "install": "^0.13.0",
    "openid-client": "^5.7.0",
    "passport": "^0.7.0",
    "react-select": "^5.8.1",
    "typescript": "^5.6.2",
    "typesense": "^1.8.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "4",
    "@types/passport": "^1.0.16",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2"
  }
}
