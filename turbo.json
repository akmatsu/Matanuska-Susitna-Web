{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "!.next/cache/**", ".keystone/**"]
    },
    "migrate": {},
    "migrations": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "codegen": {
      "cache": false,
      "persistent": false
    },
    "codegen:watch": {
      "cache": false,
      "persistent": true
    }
  }
}
